/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Tam-CO's code here:
    Not(in=a, out=nota);
    Not(in=b, out=notb);
    And(a=nota, b=b, out=notAAndB);
    And(a=a, b=notb, out=aAndNotB);
    Or(a=notAAndB, b=aAndNotB, out=out);
}
